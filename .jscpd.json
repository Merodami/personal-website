{
  "threshold": 0,
  "reporters": ["html", "console"],
  "ignore": [
    "**/node_modules/**",
    "**/.git/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/*.min.js",
    "**/*.min.css",
    "**/vendor/**",
    "**/public/**",
    "**/.yarn/**",
    "**/lighthouse/**"
